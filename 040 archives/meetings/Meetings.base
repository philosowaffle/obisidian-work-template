filters:
  and:
    - file.tags.contains("meeting")
    - '!file.folder.contains("002 meta")'
views:
  - type: table
    name: Table
    order:
      - file.name
      - Description
      - Meeting Attendees
      - Related Projects
      - Meeting Datetime
    sort:
      - property: file.name
        direction: DESC
    columnSize:
      note.Description: 394
