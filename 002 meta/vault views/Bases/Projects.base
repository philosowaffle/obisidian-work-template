filters:
  and:
    - file.tags.contains("project")
    - '!file.folder.startsWith("002 meta")'
views:
  - type: table
    name: All
    order:
      - file.name
      - Description
      - Project Next Steps
      - Project Status
      - Project Stakeholders
    sort:
      - property: Project Next Steps
        direction: ASC
    columnSize:
      note.Description: 343
      note.Project Next Steps: 340
    rowHeight: medium
