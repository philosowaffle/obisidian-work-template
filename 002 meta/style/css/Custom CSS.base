filters:
  and:
    - file.tags.contains("style/css")
views:
  - type: table
    name: All
    order:
      - file.name
      - Enabled
      - cssclasses
      - Source File
    columnSize:
      note.cssclasses: 445
  - type: table
    name: Enabled
    filters:
      and:
        - Enabled == "✔️"
    order:
      - file.name
      - Enabled
